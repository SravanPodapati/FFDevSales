<?xml version="1.0" encoding="UTF-8"?>
<HomePageComponent xmlns="http://soap.sforce.com/2006/04/metadata">
    <body>&lt;script&gt; 	if (window.addEventListener) {  		window.addEventListener(&apos;load&apos;, removeNewbutton, false); 		window.addEventListener(&apos;load&apos;, removeCreateNewAccount, false); 	} else if (window.attachEvent) {  		window.attachEvent(&apos;onload&apos;, removeNewbutton);		window.attachEvent(&apos;onload&apos;, removeCreateNewAccount);	}	function removeNewbutton(){			var toRemove = document.getElementsByName(&apos;new&apos;); 			if(document.location.href.toString().indexOf(&apos;/001/o&apos;) != -1 &amp;&amp; toRemove &amp;&amp; toRemove.length){	 			toRemove[0].onclick = SearchAccounts;				} 	}	function removeCreateNewAccount(){		var els = document.getElementsByTagName(&quot;a&quot;);		for (var i = 0, l = els.length; i &lt; l; i++) {			var el = els[i];			if(el.href.toString().indexOf(&apos;/setup/ui/recordtypeselect.jsp?ent=Account&apos;) &gt; -1){				el.href = &apos;/apex/accountsearchscreen&apos;;			}		}	}	function SearchAccounts(){				var s1 = document.URL;			var s2 = s1.indexOf(&apos;salesforce.com&apos;);			var s3 = s1.substring(0, s2);			s3 = s3.concat(&apos;salesforce.com/apex/accountsearchscreen&apos;);			window.open(s3,&apos;_self&apos;);	}&lt;/script&gt;</body>
    <pageComponentType>htmlArea</pageComponentType>
    <width>narrow</width>
</HomePageComponent>
