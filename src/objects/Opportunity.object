    <validationRules>
        <fullName>AgencyRestrictions</fullName>
        <active>true</active>
        <description>If main account is an Advertiser and Type Client, the secondary account on the opp should always be populated.</description>
        <errorConditionFormula>!(BEGINS(Record_Type__c,&apos;Wowcher&apos;) )&amp;&amp; 
Account_Record_Type__c = &apos;Advertiser&apos; &amp;&amp; ISPICKVAL(Account.Type, &apos;Client&apos;) &amp;&amp; Agency__c = &apos;&apos;
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Agency__c</errorDisplayField>
        <errorMessage>If main account is an Advertiser and of Type Client, the Agency must be set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BrandBrandsWithClientParentRequireAgency</fullName>
        <active>true</active>
        <description>Brand type records that have Client parent accounts must have the agency set on the pitch</description>
        <errorConditionFormula>!(BEGINS(Record_Type__c,&apos;Wowcher&apos;) )&amp;&amp; 
Account_Record_Type__c = &apos;Advertiser&apos; 
&amp;&amp; ISPICKVAL( Account.Type, &quot;Brand&quot;) 
&amp;&amp; ISPICKVAL( Account.Parent.Type, &apos;Client&apos;) &amp;&amp; Agency__c = &apos;&apos; 
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Agency__c</errorDisplayField>
        <errorMessage>Brand account pitches with Client parent accounts must have the Agency set on the pitch</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CannotCloseWonOpportunityWithNoProduct</fullName>
        <active>true</active>
        <errorConditionFormula>!BEGINS(Record_Type__c,&apos;Wowcher&apos;)  &amp;&amp; Record_Type__c != &apos;PopMac&apos; &amp;&amp; 
IsClosed &amp;&amp; IsWon &amp;&amp; CreationMethod__c != &apos;Pitch Hub&apos; &amp;&amp; CreationMethod__c !=  &apos;Multiple Opportunities Multiple Products&apos; &amp;&amp; (CountOfCustomerRef4__c  = 0)&amp;&amp;(CountOfCustomerRef2__c=0) &amp;&amp; (CountOfCustomerRef3__c=0) &amp;&amp; (CountOfCustomerRef1__c=0) &amp;&amp; (CountOfCustomerRef5__c=0)
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorMessage>You cannot Close a Won Pitch/Media Lead without at least one Product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckAdvertiserIsSet</fullName>
        <active>true</active>
        <description>Advertiser needs to be set before the Stage reaches Final Negotiation</description>
        <errorConditionFormula>!(BEGINS(Record_Type__c,&apos;Wowcher&apos;) )
&amp;&amp;  !(Record_Type__c = &apos;PopMac&apos;) 
&amp;&amp; Account_Record_Type__c = &apos;Agency&apos; 
&amp;&amp;  ( ISPICKVAL(StageName,&apos;Final Negotiations&apos;)   || ISPICKVAL(StageName,&apos;Closed Won&apos;) || ISPICKVAL(StageName,&apos;Closed Carrying&apos;))   
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>An Agency cannot be used as an Advertiser pass the Final Negotiations stage. Please choose an Advertiser.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FollowUpDueDateGreaterThanToday</fullName>
        <active>true</active>
        <errorConditionFormula>IF(!ISBLANK(Followup_Due_Date__c), IF( Followup_Due_Date__c&lt; TODAY(),TRUE,FALSE),FALSE)
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorMessage>Follow-Up Due Date must be greater than Today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FollowUpReminderLessThanFollowupDueDate</fullName>
        <active>true</active>
        <errorConditionFormula>IF(!ISBLANK(Followup_Due_Date__c), IF(!ISBLANK( Followup_Reminder__c), IF(Followup_Due_Date__c&lt;DATEVALUE(Followup_Reminder__c),TRUE,FALSE),FALSE),FALSE)
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorMessage>Follow-Up Reminder must be before the Follow-Up Due Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Free_Ad_Value_Gt_0_For_Agency_Contract</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Free_Ad__c, &quot;Agency Contract value&quot;) &amp;&amp;  Free_Ad_Value__c  &lt;= 0
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Free_Ad_Value__c</errorDisplayField>
        <errorMessage>Free Ad Value must be greater than 0 for Free Ads of type Agency Contract value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IfTypeOfCallIsSelectedFollowUpTaskReq</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISPICKVAL( Type_of_Call__c, &quot;Inbound Sales Call&quot;),ISPICKVAL( Type_of_Call__c, &quot;Outbound Sales Call&quot;),ISPICKVAL( Type_of_Call__c, &quot;New Business Call&quot;)),
OR(ISBLANK( Followup_Subject__c ), ISBLANK( Followup_Due_Date__c )))
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorMessage>If Type of Call is selected then Follow-up task is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IfTypeOfCallIsSelectedThenPopmacRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISPICKVAL( Type_of_Call__c, &quot;Inbound Sales Call&quot;),ISPICKVAL( Type_of_Call__c, &quot;Outbound Sales Call&quot;),ISPICKVAL( Type_of_Call__c, &quot;New Business Call&quot;)),
OR(ISBLANK(Open__c),ISBLANK(Prepare__c), ISBLANK(Probe__c), ISBLANK(Match__c),ISBLANK(Agreement__c), ISBLANK(Close__c)))
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorMessage>If Type of Call is selected then all of POPMAC is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LostToCompetitor</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(	Loss_Reason__c, &apos;Lost to Competitor&apos;) &amp;&amp; ISBLANK(TEXT( Lost_to_which_Competitor__c))
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Lost_to_which_Competitor__c</errorDisplayField>
        <errorMessage>If Loss Reason is &apos;Lost to Competitor&apos;, competitor must be selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LostToOtherCompetitor</fullName>
        <active>true</active>
        <description>Other Competitor needs to be populated when Lost to which competitor is set to &apos;Other - X&apos;.</description>
        <errorConditionFormula>( 
CONTAINS(TEXT(Lost_to_which_Competitor__c), &apos;Other Media&apos;)
)

&amp;&amp; ISBLANK(Other_Competitor__c)

&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Other_Competitor__c</errorDisplayField>
        <errorMessage>Other Competitor needs to be populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MandatoryFieldsForPitchRegion</fullName>
        <active>true</active>
        <description>Wowcher: Deal Region is Mandatory for EComm</description>
        <errorConditionFormula>Record_Type__c = &apos;Wowcher Merchant (ecomm)&apos;
&amp;&amp;  ISBLANK( TEXT(Deal_Region__c) )
