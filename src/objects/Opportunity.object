(Account.CountAdvanceAccountMappings__c=0 || (!ISBLANK( Agency__c ) &amp;&amp; Agency__r.CountAdvanceAccountMappings__c=0)) &amp;&amp; CountOfCustomerRef2__c&gt;0, 

