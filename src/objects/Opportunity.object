    </validationRules>
    <validationRules>
        <fullName>MandatoryFieldsForPitchWhenCategoryFood</fullName>
        <active>true</active>
        <description>Restaurant Payment Terms must be chosen when category is food &amp; drink.</description>
        <errorConditionFormula>BEGINS(Record_Type__c,&apos;Wowcher&apos;)
&amp;&amp;Record_Type__c = &apos;Wowcher Merchant (instore)&apos; 
 &amp;&amp;
ISPICKVAL(Category__c,&apos;Food &amp; Drink&apos;) &amp;&amp;
ISBLANK(TEXT( Restaurant_Payment_Terms__c ) )
&amp;&amp;!ISPICKVAL(StageName, &apos;open&apos;)</errorConditionFormula>
        <errorDisplayField>Restaurant_Payment_Terms__c</errorDisplayField>
        <errorMessage>Please choose the Restaurant Payment Terms.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MandatoryFieldsForPitchWhenRegionTravel</fullName>
        <active>false</active>
        <description>DO NOT ACTIVATE</description>
        <errorConditionFormula>Record_Type__c = &apos;Wowcher Merchant (instore)&apos;
&amp;&amp; ISPICKVAL( Deal_Region__c , &apos;Travel&apos;) 
&amp;&amp;  ( ISNULL(Book_By__c)
 ||  ISNULL(Number_Of_Rooms__c ))
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorMessage>If the Deal Region is Travel then both the Book By Date and the Number of Rooms have to be populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MandatoryFieldsForPitchWhenStageApproved</fullName>
        <active>true</active>
        <description>Mandatory fields  if stage = approved awaiting or approved and scheduled, analyst must fill in date approved, approval points and predicted units</description>
        <errorConditionFormula>BEGINS(Record_Type__c,&apos;Wowcher&apos;) &amp;&amp;
OR(ISPICKVAL(StageName , &apos;Approved - Awaiting Scheduling&apos;) ,ISPICKVAL(StageName , &apos;Approved &amp; Scheduled&apos;)) &amp;&amp;
OR(
ISNULL(Date_Approved__c), ISNULL(Approval_Point__c),
Predicted_Units__c=0, ISNULL(Predicted_Units__c))
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Date Approved, Approval Points and Predicted Units must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OppAd_info_when_not_newspaper</fullName>
        <active>true</active>
        <errorConditionFormula>!(CONTAINS(Record_Type__c,&apos;Wowcher&apos;)) &amp;&amp; 
!(ISBLANK( Date_of_Publication__c ) &amp;&amp;
ISBLANK( Brand_Product__c ) &amp;&amp;
ISBLANK(Size_of_ad__c) &amp;&amp;
ISBLANK( Page__c ))
&amp;&amp;
! ISPICKVAL(LeadSource,&apos;Newspaper&apos;)
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorMessage>Date of Publication, Competitor, Brand, Page and Size must be empty when Lead Source is not &apos;Newspaper&apos;.</errorMessage>
    </validationRules>
