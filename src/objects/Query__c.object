    <validationRules>
        <fullName>CannotChangeSalesInvoice</fullName>
        <active>true</active>
        <description>Cannot change the Sales Invoice once it has been saved.</description>
        <errorConditionFormula>ISCHANGED( SalesInvoice__c )</errorConditionFormula>
        <errorDisplayField>SalesInvoice__c</errorDisplayField>
        <errorMessage>You cannot change the Sales Invoice once a record has been created. You must delete and create a new Query.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CreditControllerInsufficientAuthority</fullName>
        <active>true</active>
        <description>The Credit Controller approving the Query to be passed must have sufficient authority – Total Query Amount &lt;= Credit Approvers credit limit on his/her User record</description>
        <errorConditionFormula>AND( CreditControllerApproved__c , ISCHANGED (CreditControllerApproved__c),OR( SterlingTotalQueryAmount__c &gt; $User.CreditApprovalLimit__c, ISBLANK ($User.CreditApprovalLimit__c)))</errorConditionFormula>
        <errorMessage>You have insufficient authority to pass this Query</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CustomerContactRequiredForActiveQuery</fullName>
        <active>true</active>
        <description>A Customer Contact is required when a Query is anything other than Draft.</description>
        <errorConditionFormula>!ISPICKVAL(Status__c, &apos;Draft&apos;) &amp;&amp; ISBLANK(CustomerContact__c)</errorConditionFormula>
        <errorDisplayField>CustomerContact__c</errorDisplayField>
        <errorMessage>You must specify a Customer Contact.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SalesApprovalStatusMustBeApproved</fullName>
        <active>true</active>
        <description>If the Status is Pending Credit Approval or Query Passed then the Sales Approval Status must be Approved</description>
        <errorConditionFormula>AND( NOT( SalesApprovalStatus__c  = &apos;Approved&apos;), OR( ISPICKVAL( Status__c, &apos;Pending Credit Controller Approval&apos;), ISPICKVAL( Status__c, &apos;Query Passed&apos;)))</errorConditionFormula>
        <errorDisplayField>SalesApprover__c</errorDisplayField>
        <errorMessage>If the Status is Pending Credit Controller Approval or Query Passed then the Sales Approval Status must be Approved – Sales Approver is blank or has insufficient authority</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ToBeAllocatedQueryAmountMustBeNil</fullName>
        <active>true</active>
        <description>If the Status is anything other than Open or Sales Approval Received then the To Be Allocated Query Amount must be nil</description>
        <errorConditionFormula>AND( ABS( ToBeAllocatedQueryAmount__c ) &gt;1, NOT(OR(ISPICKVAL(Status__c, &apos;Draft&apos;),ISPICKVAL(Status__c, &apos;Sales Approval Received&apos;))))</errorConditionFormula>
        <errorDisplayField>TotalQueryAmount__c</errorDisplayField>
        <errorMessage>If the Status is anything other than Open or Sales Approval Received then the To Be Allocated Query Amount must be nil</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TotalQueryAmountCannotExceedNetInvoice</fullName>
        <active>true</active>
        <description>The Total Query Amount cannot Exceed the Net Invoice Value.</description>
        <errorConditionFormula>TotalQueryAmount__c &gt;  NetTotal__c</errorConditionFormula>
        <errorDisplayField>TotalQueryAmount__c</errorDisplayField>
        <errorMessage>The Total Query Amount cannot exceed the Invoice Net total.</errorMessage>
    </validationRules>
