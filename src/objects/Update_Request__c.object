    <validationRules>
        <fullName>AccBillingAddrRequiredOnAgencyDirectAdv</fullName>
        <active>true</active>
        <description>only for accounts</description>
        <errorConditionFormula>!( $Setup.No_Validations__c.Flag__c ) &amp;&amp; 
Account__c != null &amp;&amp; 
OR((Account__r.RecordType.Name = &apos;Advertiser&apos; &amp;&amp; 
	if(!Type_S__c, OR(ISPICKVAL(Account__r.Type, &apos;Direct Advertiser&apos;),ISPICKVAL(Account__r.Type, &apos;Private Advertiser&apos;),ISPICKVAL(Account__r.Type, &apos;Charity&apos;)), 
		OR(Type__c==&apos;Direct Advertiser&apos;,Type__c==&apos;Private Advertiser&apos;,Type__c==&apos;Charity&apos;))
),
(Account__r.RecordType.Name = &apos;Agency&apos; &amp;&amp; 
if(!Type_S__c, ISPICKVAL(Account__r.Type, &apos;Billing Agency&apos;), Type__c==&apos;Billing Agency&apos;))
)&amp;&amp;
if(!Billing_Country_S__c, lower(Account__r.BillingCountry) != &apos;ireland&apos;, lower(Billing_Country__c) != &apos;ireland&apos;)&amp;&amp;
if(!Billing_Street_S__c, ISBLANK(Account__r.BillingStreet), ISBLANK(Billing_Street__c))</errorConditionFormula>
        <errorMessage>Billing Street is mandatory on Billing Agencies, Direct Advertisers, Private Advertisers and Charity unless they&apos;re based in Ireland.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AccBillingPostCodeReqOnAgencyDirectAdv</fullName>
        <active>true</active>
        <description>only for Accounts</description>
        <errorConditionFormula>!( $Setup.No_Validations__c.Flag__c ) &amp;&amp; 
Account__c != null &amp;&amp; 
OR((Account__r.RecordType.Name = &apos;Advertiser&apos; &amp;&amp; 
if(!Type_S__c, OR(ISPICKVAL(Account__r.Type, &apos;Direct Advertiser&apos;),ISPICKVAL(Account__r.Type, &apos;Private Advertiser&apos;),ISPICKVAL(Account__r.Type, &apos;Charity&apos;)), 
OR(Type__c==&apos;Direct Advertiser&apos;,Type__c==&apos;Private Advertiser&apos;,Type__c==&apos;Charity&apos;)) 
), 
(Account__r.RecordType.Name = &apos;Agency&apos; &amp;&amp; 
if(!Type_S__c, ISPICKVAL(Account__r.Type, &apos;Billing Agency&apos;), Type__c==&apos;Billing Agency&apos;) 
)) &amp;&amp; 
if(!Billing_Zip_Postal_Code_S__c, ISBLANK(Account__r.BillingPostalCode), ISBLANK(Billing_Zip_Postal_Code__c)) &amp;&amp; 
if(!Billing_Country_S__c, lower(Account__r.BillingCountry) != &apos;ireland&apos;, lower(Billing_Country__c) != &apos;ireland&apos;)</errorConditionFormula>
        <errorMessage>Billing Postcode is mandatory on Billing Agencies, Direct Advertisers, Private Advertisers and Charity unless they&apos;re based in Ireland.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AccountCheckPhoneFormat</fullName>
        <active>true</active>
        <description>only for accounts</description>
        <errorConditionFormula>!( $Setup.No_Validations__c.Flag__c ) &amp;&amp; 
Account__c != null &amp;&amp; 
!REGEX( Phone_Number__c , &apos;[0-9-()+/ ]*&apos;)</errorConditionFormula>
        <errorMessage>Phone Field shouldn&apos;t allow text to be added.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AccountTypeChangeValidation</fullName>
        <active>true</active>
        <description>only for Accounts</description>
        <errorConditionFormula>!( $Setup.No_Validations__c.Flag__c ) &amp;&amp; 
Account__c != null &amp;&amp;If( 
( 
Account__r.RecordType.Name=&apos;Advertiser&apos;&amp;&amp; 
Type_S__c&amp;&amp; 
( 
Not(ISBLANK(Account__r.CCIMailCustID__c))|| 
Not(ISBLANK(Account__r.CCIMetroCustID__c)) 
) 
), 
(If( 
OR( 
( 
ISPICKVAL(Account__r.Type,&apos;Client&apos;)&amp;&amp; 
(Type__c == &apos;Direct Advertiser&apos;) 
), 
( 
ISPICKVAL(Account__r.Type,&apos;Client&apos;)&amp;&amp; 
(Type__c == &apos;Brand&apos;) 
), 
( 
ISPICKVAL(Account__r.Type,&apos;Brand&apos;)&amp;&amp; 
(Type__c == &apos;Client&apos;) 
), 
( 
ISPICKVAL(Account__r.Type,&apos;Brand&apos;)&amp;&amp; 
(Type__c == &apos;Direct Advertiser&apos;) 
) 
), 
false, 
true 
) 
), 
false 
)</errorConditionFormula>
        <errorMessage>Type can not be changed after an account is synchronised with CCI.</errorMessage>
    </validationRules>
