    <validationRules>
        <fullName>AdvertiserOrDescriptionMustBeCompleted</fullName>
        <active>true</active>
        <description>When saving a Media Lead either the Advertiser or the Advertiser Description field must be completed</description>
        <errorConditionFormula>(ISBLANK(Company__c) &amp;&amp; ISBLANK(Advertiser_Description__c)) &amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Company__c</errorDisplayField>
        <errorMessage>When saving a Media Lead either the Advertiser or the Advertiser Description field must be completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CloLostReaOtherThenClosedLostReasonOther</fullName>
        <active>true</active>
        <description>When Closed Lost Reason is Other, then &apos;Closed Reason Description&apos; is required.</description>
        <errorConditionFormula>ISPICKVAL(Closed_Lost_Reason__c,&apos;Other&apos;) &amp;&amp; ISBLANK( Closed_Lost_Reason_Other__c )
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Closed_Lost_Reason_Other__c</errorDisplayField>
        <errorMessage>When Closed Lost Reason is Other, then &apos;Closed Reason Description&apos; is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LockDownMediaLeadStatusAfterConverting</fullName>
        <active>true</active>
        <description>The Status field cannot be changed after a Media Lead has been Closed-Converted as a Pitch has already been created.</description>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE(Status__c), &apos;Closed-Converted&apos;) &amp;&amp;  ISCHANGED(Status__c)</errorConditionFormula>
        <errorMessage>The Status field cannot be changed after a Media Lead has been &apos;Closed-Converted&apos; and a new Pitch has already been created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LostToWhichCompetitorIsReqWhenReIsLost</fullName>
        <active>true</active>
        <description>If Lost Reason is &apos;Lost to Competitor&apos;, then &apos;Lost to Which Competitor&apos; must be selected.</description>
        <errorConditionFormula>ISPICKVAL(Closed_Lost_Reason__c, &apos;Lost to Competitor&apos;) &amp;&amp; ISNULL(Lost_to_which_Competitor__c) 
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Lost_to_which_Competitor__c</errorDisplayField>
        <errorMessage>If Lost Reason is &apos;Lost to Competitor&apos;, then &apos;Lost to Which Competitor&apos; must be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MediaLeadLockReqFieldsStatusConverted</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Status__c,&apos;Closed-Converted&apos;) &amp;&amp; ( ISCHANGED(Company__c) ||  ISCHANGED(Agency__c) || ISCHANGED(Advertiser_Description__c) || ISCHANGED(Closed_Lost_Reason__c) || ISCHANGED(Closed_Lost_Reason_Other__c) || ISCHANGED(Colour__c) || ISCHANGED(Competitor__c) || ISCHANGED(Date_of_Publication__c) || ISCHANGED(Lead_Source__c) || ISCHANGED(Position__c) || ISCHANGED(Size_of_Ad__c))</errorConditionFormula>
        <errorMessage>You cannot change any of the fields once the Media Lead status is set to Closed-Converted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReasonIsNeededIfStatusIsClosedLost</fullName>
        <active>true</active>
        <description>Closed Lost Reason is needed when setting Status to Closed Lost.</description>
        <errorConditionFormula>ISBLANK(TEXT(Closed_Lost_Reason__c)) &amp;&amp; ISPICKVAL( Status__c , &apos;Closed-Lost&apos;)
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Closed_Lost_Reason__c</errorDisplayField>
        <errorMessage>Closed Lost Reason is needed when setting Status to Closed Lost.</errorMessage>
    </validationRules>
