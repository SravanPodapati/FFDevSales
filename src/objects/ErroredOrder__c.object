        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)} 

var idArray = {!GETRECORDIDS($ObjectType.ErroredOrder__c)};
var errorResponse = &apos;&apos;; 
if (idArray [0] == null) { 
        alert(&quot;Please select at least one record to create order.&quot;);     
    } else { 
    for (var a=0; a&lt;idArray.length; a++) {   

      errorResponse +=  sforce.apex.execute(&quot;WebServices&quot;,&quot;CreateOrdernItemsFromErroredOrder&quot;, 
{erroredOrder:idArray[a]}); 
}

window.location.href=&quot;/a1k?fcf=00BL0000000ONnX&quot;; 



}</url>
