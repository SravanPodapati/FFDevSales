    <validationRules>
        <fullName>AnEffectTaskCannotBeSavedWithoutPopmac</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISBLANK(Open__c),ISBLANK(Prepare__c), ISBLANK(Probe__c), ISBLANK(Match__c),ISBLANK(Agreement__c), ISBLANK(Close__c)) 
&amp;&amp; !( $Setup.No_Validations__c.Flag__c ) 
&amp;&amp; Activity_Record_Type__c = &apos;London_Classified_Task&apos; 
&amp;&amp; ISPICKVAL(Outcome__c, &apos;Effective&apos;)
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorMessage>An Effective call cannot be saved without the POPMAC process.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CannotSaveTaskWithoutOutcome</fullName>
        <active>true</active>
        <errorConditionFormula>/*((Activity_Record_Type__c = &apos;London_Classified_Task&apos; )|| BEGINS($User.Profile_Name__c, &apos;Mail&apos;)) 
&amp;&amp;*/

(!BEGINS($User.Profile_Name__c, &apos;System&apos;)&amp;&amp;!BEGINS($User.Profile_Name__c, &apos;Wowcher&apos;)&amp;&amp; !(  Activity_Record_Type__c  =&apos;Marketing_Task&apos;))&amp;&amp;
CONTAINS( $Profile.Name, &apos;Class&apos;)  &amp;&amp;
 ISPICKVAL( Status , &apos;Completed&apos;) &amp;&amp;
ISPICKVAL(Outcome__c, &apos;&apos;) 
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Outcome__c</errorDisplayField>
        <errorMessage>Please select an OutCome</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OutcomeForCompletedTasks</fullName>
        <active>false</active>
        <description>Outcome field needs to be set when status is Completed.</description>
        <errorConditionFormula>ISPICKVAL(Status, &apos;Completed&apos;) &amp;&amp; ISPICKVAL(Outcome__c, &apos;&apos;) 

&amp;&amp; !($Setup.No_Validations__c.Flag__c)</errorConditionFormula>
        <errorDisplayField>Outcome__c</errorDisplayField>
        <errorMessage>Outcome field needs to be set when status is Completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TypeMustBePopulated</fullName>
        <active>true</active>
        <errorConditionFormula>/*(BEGINS($User.Profile_Name__c, &apos;London&apos;)|| BEGINS($User.Profile_Name__c, &apos;Mail&apos;))&amp;&amp;
*/
(!BEGINS($User.Profile_Name__c, &apos;System&apos;)&amp;&amp;!BEGINS($User.Profile_Name__c, &apos;Wowcher&apos;)&amp;&amp;!( Activity_Record_Type__c =&apos;Marketing_Task&apos;)&amp;&amp;!( Activity_Record_Type__c =&apos;Sales_Ledger_Task&apos;))&amp;&amp;
CONTAINS( $Profile.Name, &apos;Class&apos;) &amp;&amp; 
ISPICKVAL( Status , &apos;Completed&apos;) &amp;&amp; 
ISPICKVAL(Type__c, &apos;&apos;)
&amp;&amp; !( $Setup.No_Validations__c.Flag__c )</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>Please enter a Type</errorMessage>
    </validationRules>
