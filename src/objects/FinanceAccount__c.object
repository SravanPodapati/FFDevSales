{
   
 window.parent.location.href=&quot;/a1h/e?Name=&quot;+acct.name+&quot;&amp;CF00ND0000005jggc=&quot;+acct.name+&quot;&amp;CF00ND0000005jggc_lkid=&quot;+acct.id+&quot;&amp;retURL=&quot;+acct.id;
}
else if(acct.type=&quot;Brand&quot; &amp;&amp; (acct.phone==null || acct.phone==&quot;&quot; ||acct.billingPostcode==null || acct.billingPostcode==&quot;&quot;||acct.billingStreet==null || acct.billingStreet==&quot;&quot;))
